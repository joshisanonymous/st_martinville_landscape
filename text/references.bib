@article{rickford_rappin_1991,
	title = {Rappin on the copula coffin: {Theoretical} and methodological issues in the analysis of copula variation in {African}-{American} {Vernacular} {English}},
	volume = {3},
	issn = {1469-8021, 0954-3945},
	shorttitle = {Rappin on the copula coffin},
	url = {http://www.cambridge.org/core/journals/language-variation-and-change/article/rappin-on-the-copula-coffin-theoretical-and-methodological-issues-in-the-analysis-of-copula-variation-in-africanamerican-vernacular-english/6C25073E7DAFDFAB8490572E4E43B2A6},
	doi = {10.1017/S0954394500000466},
	abstract = {We explore two unresolved methodological issues in the study of copula variation in African-American Vernacular English, assessing their quantitative and theoretical consequences via multiple variable rule analyses of data from East Palo Alto, California. The first is whether is- contraction and deletion should be considered separately from that of are. We conclude that it should not, because the quantitative conditioning is almost identical for the two forms, and a combined analysis offers analytical advantages. The second issue is whether the alternative methods that previous researchers have used to compute the incidence of “contraction” or “deletion” (“Labov Contraction and Deletion,” “Straight Contraction and Deletion,” “Romaine Contraction”) fundamentally affect the results. We conclude that they do, especially for contraction. We also discuss implications of our analysis for two related issues: the ordering of contraction and deletion in the grammar, and the presence of age-grading or change in progress in East Palo Alto.},
	language = {en},
	number = {1},
	urldate = {2020-01-14},
	journal = {Language Variation and Change},
	author = {Rickford, John R. and Ball, Arnetha and Blake, Renee and Jackson, Raina and Martin, Nomi},
	month = mar,
	year = {1991},
	keywords = {Read, sociolinguistics, language change, language variation, methods, verbs, african-americans},
	pages = {103--132},
	file = {Rickford et al. - 1991 - Rappin on the copula coffin Theoretical and metho.pdf:C\:\\Users\\josh8\\Zotero\\storage\\K8CTC76S\\Rickford et al. - 1991 - Rappin on the copula coffin Theoretical and metho.pdf:application/pdf;Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\JKHBAVNE\\6C25073E7DAFDFAB8490572E4E43B2A6.html:text/html},
}

