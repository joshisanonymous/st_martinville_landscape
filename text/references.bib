	keywords = {language change, english, linguistics, lexical variation},
	pages = {199--221},
	file = {Full Text PDF:C\:\\Users\\josh8\\Zotero\\storage\\CE67HW4I\\Stratton - 2023 - Where did wer go Lexical variation and change in .pdf:application/pdf},
}

