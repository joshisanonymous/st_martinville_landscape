	keywords = {sociolinguistics, language change, french, linguistics, language variation, pronouns},
	pages = {164--191},
	file = {Full Text PDF:C\:\\Users\\josh8\\Zotero\\storage\\QYRVSAZ4\\Coveney - 2003 - 'Anything you can do, tu can do better' tu and vo.pdf:application/pdf;Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\X75KGZJQ\\1467-9481.html:text/html},
