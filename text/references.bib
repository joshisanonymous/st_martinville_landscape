	file = {ScienceDirect Full Text PDF:C\:\\Users\\josh8\\Zotero\\storage\\M4EPPPQ7\\Pardo et al. - 2018 - A comparison of phonetic convergence in conversati.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\QTZE7XGJ\\S0095447017300992.html:text/html},
