	keywords = {french, linguistics, morphology, language variation, dialectology, gender, language policy},
	pages = {486--513},
	file = {Full Text PDF:C\:\\Users\\josh8\\Zotero\\storage\\34TPB8HX\\Dow et Drouin - 2023 - Tracing the evolution of the gender of “COVID-19” .pdf:application/pdf},
}

