@article{fasold_ebonic_2000,
	title = {Ebonic {Need} {Not} {Be} {English}},
	volume = {25},
	issn = {0171-5410},
	url = {https://www.jstor.org/stable/43025634},
	abstract = {This article is dedicated to Prof. Dr. Alwin Fill of the Institut für Anglistik, Karl Franzens-Universität Graz. Prof. Fill showed me extraordinary kindness during my term in Graz as Fulbright guest professor, and I will never forget all that he did to make my stay a pleasant one. Conversations we had, and his work which I have read on ecological linguistics, makes me realize that an ecological linguistic approach would be an insightful way to examine the place of Ebonic in its social setting in the United States; why it differs as much as it does from other kinds of American English, why it does not differ more, and why it is being so robustly maintained. I regret that time has not allowed my to include an ecological perspective in the present article. Another strand of Professor Fill's work, on tension in linguistic systems, contributes what I believe is a key concept in the proper understanding of the distribution of reflexives in various languages, another of area of my research. I am pleased to offer the present work as a token of my appreciation.},
	number = {2},
	urldate = {2020-01-15},
	journal = {AAA: Arbeiten aus Anglistik und Amerikanistik},
	author = {Fasold, Ralph W.},
