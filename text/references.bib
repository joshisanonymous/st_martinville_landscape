@inproceedings{danescu-niculescu-mizil_no_2013,
	address = {New York, NY, USA},
	series = {{WWW} '13},
	title = {No {Country} for {Old} {Members}: {User} {Lifecycle} and {Linguistic} {Change} in {Online} {Communities}},
