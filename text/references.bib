@article{dubois_lets_1998,
	title = {Let's tink about dat: {Interdental} fricatives in {Cajun} {English}},
	volume = {10},
	issn = {1469-8021, 0954-3945},
	shorttitle = {Let's tink about dat},
	url = {http://www.cambridge.org/core/journals/language-variation-and-change/article/lets-tink-about-dat-interdental-fricatives-in-cajun-english/89ED3FD30436D352AC170F67AA708996},
	doi = {10.1017/S0954394500001320},
	abstract = {The English of bilingual Cajuns living in southern Louisiana has been pejoratively depicted as an accented English; foremost among the stereotypes of Cajun English is the use of tink and dat for think and that. We present a variationist study of /θ/ and /ð/ in the speech of bilingual Cajuns in St. Landry Parish. The results show a complex interrelationship of age, gender, and social network. One of the major findings is a v-shaped age pattern rather than the regular generational model that is expected. The older generation use more of the dental variants [t, d] than all others, the middle-aged dramatically decrease their use, but the young show a level of usage closer to the old generation. The change is attributed to both language attrition and the blossoming of the Cajun cultural renaissance. Interestingly, neither young men in open networks nor women of all ages in open networks follow the v-shaped age pattern. In addition, they show opposite directions of change: men in open networks lead the change to [d], whereas women in open networks drop the dental variants of [t, d] almost entirely.},
	language = {English},
	number = {3},
	urldate = {2018-10-10},
	journal = {Language Variation and Change},
	author = {Dubois, Sylvie and Horvath, Barbara M.},
	month = oct,
	year = {1998},
	keywords = {sociolinguistics, language change},
	pages = {245--261},
	file = {Dubois et Horvath - 1998 - Let's tink about dat Interdental fricatives in Ca.pdf:C\:\\Users\\josh8\\Zotero\\storage\\42YD4RCA\\Dubois et Horvath - 1998 - Let's tink about dat Interdental fricatives in Ca.pdf:application/pdf;Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\MURS4KAM\\89ED3FD30436D352AC170F67AA708996.html:text/html},
