@article{dorais_discourse_2000,
	title = {Discourse and {Identity} in the {Baffin} {Region}},
	volume = {37},
	issn = {0066-6939},
	url = {http://www.jstor.org.proxy.bibliotheques.uqam.ca:2048/stable/40316532},
	abstract = {This article describes discourse practices (the culturally relevant ways of communicating) in Iqaluit and Igloolik, two predominantly Inuit communities of Nunavut's Baffin Region. Based on observation and interviews with Inuit, anglophone, and francophone respondents, it seeks to understand how language behavior may be understood as an expression of Inuit identity. The description outlines a few salient points of the present-day linguistic situation in the Baffin Region: the role played by English as a lingua franca (common language); an apparent correlation between schooling and a decreasing use of Inuktitut in daily life (high school students and educated adults speak much less Inuktitut than less educated individuals); and, at the same time, the strength of the aboriginal language, which is still widely spoken and routinely transmitted to the new Inuit generations. Opinions of Inuit informants on the usefulness and importance of each language reveal the existence of a dichotomy of sorts. English is deemed more useful than Inuktitut, because it paves the way to better employment and opens a window on the wider world, but Inuktitut is generally perceived as essential in defining Inuit identity. Many think that with the advent of Nunavut, Inuktitut will become a prerequisite for holding a job, thus increasing its practical usefulness.},
	number = {2},
	urldate = {2016-02-11},
	journal = {Arctic Anthropology},
	author = {Dorais, Louis-Jacques and Sammons, Susan},
	year = {2000},
	keywords = {sociolinguistics, language change},
	pages = {92--110},
	file = {Dorais et Sammons - 2000 - Discourse and Identity in the Baffin Region.pdf:C\:\\Users\\josh8\\Zotero\\storage\\IPA6KB7B\\Dorais et Sammons - 2000 - Discourse and Identity in the Baffin Region.pdf:application/pdf},
