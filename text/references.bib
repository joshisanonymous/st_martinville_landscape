@phdthesis{st-hilaire_its_2000,
	address = {Ann Arbor, United States},
	type = {Ph.{D}.},
	title = {It's our culture: {Global} process, community development and language survival in {Louisiana} and {St}. {Lucia}, {Part} {I}},
	copyright = {Copyright UMI - Dissertations Publishing 2000},
	shorttitle = {It's our culture},
	url = {http://search.proquest.com.libproxy.tulane.edu:2048/docview/304604075/abstract/1411FA8CA507248961E/1?accountid=14437},
	abstract = {The dissertation research explores the survivability of Louisiana French and St Lucian French Creole (Kweyol). Most sociolinguistic research on minority languages has focused on language death and has ignored global process. However, the continued and renewed salience of ethnicity as a social organizer gives reason to expect that the death of many minority languages may be arrested or reversed. The Louisiana and St Lucia cases are significant in that Louisiana French and Kweyol have both been the object of organized efforts at revaluation since the 1960s. The cases are also significant from a comparative, global perspective because of their integration into broader pan-cultural movements and the different positions they occupy within the world-system. This study investigates in relation to each case: (1) the global processes that are associated with ethnic revival, (2) the nature of ethnic revival and (3) the association between the ethnic revival movements, on the one hand, and patterns of language use and language attitudes, on the other.
The project uses a comparative approach to identify common themes and to isolate phenomena unique to each case. Data from secondary sources inform global process, the ethnic movements and the sociohistorical narratives of each case. Interviews with randomly selected residents and the protagonists of linguistic preservation and promotion in Louisiana and St Lucia furnish data on ethnic revival and on the sociolinguistic status of Louisiana French and Kweyol. The findings support the thesis that Louisiana French and Kweyol ethnic revival is associated with changes in the hegemonic status and the integration of the world-system. However, ethnic revival in Louisiana and St Lucia has only partially arrested the decline in the use and viability of Louisiana French and Kweyol. English remains the predominant language of social discourse in all public domains of language use, although Louisiana French and Kweyol have made some gains. Moreover, despite the revival movements, English continues to usurp social space from the ethnic languages in private domains of use as well. The greatest effect of ethnic revival on the survivability of Louisiana French and Kweyol lies in the positive changes of people's attitudes toward the languages.},
	language = {Anglais},
	urldate = {2013-10-15},
	author = {St-Hilaire, Aonghas MacThomais},
	year = {2000},
	keywords = {sociolinguistics, language change},
	file = {Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\2EC664Z6\\1.html:text/html},
}

