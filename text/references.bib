@inproceedings{stewart_anorexia_2017,
	address = {Boston, MA},
	title = {\#{Anorexia}, \#anarexia, \#anarexyia: {Characterizing} online community practices with orthographic variation},
	isbn = {978-1-5386-2715-0},
