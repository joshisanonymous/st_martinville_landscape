@article{lopez_facil_2018,
	title = {Fácil or {A} piece of cake: {Does} variability in bilingual language brokering experience affect idiom comprehension?},
	volume = {21},
	issn = {1366-7289, 1469-1841},
	shorttitle = {Fácil or {A} piece of cake},
	url = {http://www.cambridge.org/core/journals/bilingualism-language-and-cognition/article/facil-or-a-piece-of-cake-does-variability-in-bilingual-language-brokering-experience-affect-idiom-comprehension/F0887B4F0544A0CD52C15C3962443BAE},
	doi = {10.1017/S1366728917000086},
	abstract = {Compared to studies of the effects of formal training in translation, little is known about the psycholinguistic impact of the experience of informal translation, or language brokering. The present study examined this issue in the context of idiom comprehension. Bilingual adults differing in prior brokering experience read English idioms and judged whether target words presented in English or Spanish were related to the idiom's meaning. For brokers, relatedness judgments were not affected by whether the targets were in the same or different language as the idiom; however, non-brokers were faster for same-language than different-language idiom-target pairings. The findings suggest that language brokering experience facilitates idiom meaning comprehension even across language boundaries, with further differences related to idiom decomposability. More generally, the findings underscore the importance of considering systematic sources of variability in language practice among bilinguals, aside from differences related to proficiency, in theorizing effects associated with bilingualism.},
	language = {en},
	number = {2},
	urldate = {2020-03-02},
	journal = {Bilingualism: Language and Cognition},
	author = {López, Belem G. and Vaid, Jyotsna},
	month = mar,
	year = {2018},
	note = {Publisher: Cambridge University Press},
	keywords = {Read, psycholinguistics, linguistics, neurolinguistics, bilingualism},
	pages = {340--354},
	file = {López et Vaid - 2018 - Fácil or A piece of cake Does variability in bili.pdf:C\:\\Users\\josh8\\Zotero\\storage\\TUK62G2C\\López et Vaid - 2018 - Fácil or A piece of cake Does variability in bili.pdf:application/pdf;Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\FBUIBU99\\F0887B4F0544A0CD52C15C3962443BAE.html:text/html},
