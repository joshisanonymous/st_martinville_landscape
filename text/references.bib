@phdthesis{barnett_french_2011,
	title = {French immersion teachers' attitudes toward {Louisiana} varieties of {French} and the integration of such varieties in their classroom: {A} quantitative and qualitative analysis},
	shorttitle = {French immersion teachers' attitudes toward {Louisiana} varieties of {French} and the integration of such varieties in their classroom},
	url = {http://search.proquest.com.libproxy.tulane.edu:2048/llba/docview/856397377/13F5EB0C5BC47E81EE8/9?accountid=14437},
	abstract = {This mixed-method study examined French immersion teachers' attitudes toward French-related varieties of Louisiana (i.e., Cajun French and Louisiana Creole) and the extent these local varieties are integrated into their classrooms. Participants were employed as French immersion teachers during the 2008-2009 academic school year. In the past, Louisiana schools played a large role in the decline of Louisiana French and the fostering of negative attitudes toward these varieties. In fact, a slogan of the Council for the Development of French in Louisiana (CODOFIL) is L'ecole a detruit le francais, l'ecole doit le restaurer (School destroyed French, school must restore it). Research was collected in three distinct phases. During the first quantitative phase, ninety-two teachers completed a subjective reaction Matched Guise test by listening to and evaluating five different varieties of French (Cajun French, Louisiana Creole, Quebecois French, Acadian French of New Brunswick, and International French). During the second quantitative phase, ninety-one participants completed a background, attitudes, and integration questionnaire. Finally during the qualitative phase, twelve participants completed one-on-one interviews. The main findings suggest that participants in general have neutral attitudes toward Louisiana French and are open to integration and the Louisianification of the immersion curriculum but lack the content knowledge in order to teach these varieties to create the desired balance of International French and Louisiana French. Furthermore, participants felt that more efforts need to be made to hire local French immersion teachers to create more of a balance with teachers hired from abroad (i.e foreign associate teachers). Eleven main problems concerning the immersion program as did suggestions on how to make the profession more manageable emerged from the qualitative data. This study concludes that Louisiana French language and culture are minimally present in the immersion classroom. However, further efforts need to be made by teachers, administrators, grass-root organizations, and local community members if the immersion schools can be considered true supporters of language maintenance. Copies of dissertations may be obtained by addressing your request to ProQuest, 789 E. Eisenhower Parkway, P.O. Box 1346, Ann Arbor, MI 48106-1346. Telephone 1-800-521-3042; email: disspub@umi.com},
	language = {English},
	urldate = {2013-07-19},
	author = {Barnett, C. Brian},
	year = {2011},
	keywords = {sociolinguistics, language change},
	file = {Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\TEUTXPEW\\9.html:text/html},
}

@book{henry_louisiana_1997,
	title = {The {Louisiana} {French} {Movement}: {Actors} and {Actions} in {Social} {Change}},
	isbn = {0-306-45464-5},
	shorttitle = {The {Louisiana} {French} {Movement}},
	url = {http://search.proquest.com.libproxy.tulane.edu:2048/llba/docview/58368757/13F5EB0C5BC47E81EE8/13?accountid=14437},
	abstract = {The Louisiana French Movement is described from a historical perspective, noting that the Louisiana legislature created CODOFIL (Council for the Development of French in Louisiana) in 1968 to encourage the use of French. It was to be a 5-year educational program to teach Standard French to primary school students. While originally intended as a bilingual program, this proved overly ambitious \& the program now focuses on French as a second language. To implement the program, it was necessary to import foreign teachers (from France, Quebec, \& Belgium), which generated opposition to the program \& led to a debate over the need for a French education program in the Louisiana context. Assessment of the program indicated that there was an increase in the number of French speakers both in the Acadian parishes \& even more in the rest of the state. In 1990, a CODOFIL survey showed 57\% of the respondents under age 19 spoke French. The language effort was supported by increased cultural production by Cajun musicians \& writers, \& there was also an increase in the popularity of Cajun music. Cajun authors played a role in the creation of a written norm for Louisiana Creole. It is suggested that most of the state's resources were geared to the educational effort. The rest of the resources were to improve the status of French by removing institutional obstacles \& promoting ethnic pride. The movement has resulted in modest success on the educational side, created a debate between those for \& against CODOFIL, \& is attempting to build an ideological concensus. 3 Tables, 102 References. B. Gadalla},
	language = {English},
	urldate = {2013-07-19},
	author = {Henry, Jacques},
	year = {1997},
	keywords = {sociolinguistics, language change},
	file = {Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\EN5AA5I7\\13.html:text/html},
}

@phdthesis{hartmann_cri_2012,
	title = {Le cri du bayou: {The} status and promotion of the {French} language and {Cajun} music in {Louisiana}},
	shorttitle = {Le cri du bayou},
	url = {http://search.proquest.com.libproxy.tulane.edu:2048/llba/docview/1323345215/13F5EB0C5BC47E81EE8/14?accountid=14437},
	abstract = {As one of the rare Francophone regions in North America, Louisiana has a unique culture and French dialect, spawning from Acadian immigrants who came to the Louisiana colony in the latter part of the 18th century. As the Cajun identity evolved, several strong influences have shaped and formed the Cajun French language; yet, it remains in danger due to damages from a 1921 law prohibiting the use of French in Louisiana and increased exposure to Anglo-American culture. However, many efforts to promote Cajun French have been employed since 1968 and the creation of the Council for the Development of French in Louisiana (CODOFIL), especially regarding the implementation of French-language immersion programs. In order to obtain their goal of preserving the Cajun French dialect, CODOFIL first realized the need to reestablish a sense of linguistic and cultural pride in the Cajun community. By hosting the World Acadian Congress in 1999, Louisiana Cajuns were able to reinforce important ties with other French-speaking communities in Canada and worldwide, thereby establishing pride in their French heritage. This new cultural identity, coupled with the immense popularity of Cajun music in Louisiana offers another method of strengthening the precarious future of Cajun French by presenting a new way to attract younger generations to the dialect. This work will explore the status of the French language and music in Louisiana today, focusing on the ways in which a strong cultural element could provide important tools for protecting and promoting the unique Cajun French dialect. Copies of dissertations may be obtained by addressing your request to ProQuest, 789 E. Eisenhower Parkway, P.O. Box 1346, Ann Arbor, MI 48106-1346. Telephone 1-800-521-3042; e-mail: disspub@umi.com},
	language = {English},
	urldate = {2013-07-19},
	author = {Hartmann, Melissa},
	year = {2012},
	keywords = {sociolinguistics, language change},
	file = {Hartmann - 2012 - Le cri du bayou The status and promotion of the F.pdf:C\:\\Users\\josh8\\Zotero\\storage\\GDK8ER44\\Hartmann - 2012 - Le cri du bayou The status and promotion of the F.pdf:application/pdf;Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\6JPSEPZ5\\14.html:text/html},
}

@book{brown_development_1997,
	title = {The {Development} of a {Louisiana} {French} {Norm}},
	isbn = {0-306-45464-5},
	url = {http://search.proquest.com.libproxy.tulane.edu:2048/llba/docview/85656143/13F5EB0C5BC47E81EE8/16?accountid=14437},
	abstract = {The development of natural \& artificial norms \& their relation to internal variation in Louisiana French is discussed, suggesting that when developing artificial norms through, eg, school curriculum decisions, naturally occurring norm development should be taken into account. A typology of language planning processes describes corpus planning, changes introduced into the structure of a language (eg, proposed spelling changes), \& status planning, changes in language use resulting in an altered status. The development of particular norms in Louisiana French has changed from the 1960s focus on the French of France to more effort to incorporate Louisiana French into the school curriculum. It is suggested that language planners must view minority languages as a resource, particularly since recent research indicates that Louisiana French speakers wish to identify themselves as Louisiana French. 49 References. B. Gadalla},
