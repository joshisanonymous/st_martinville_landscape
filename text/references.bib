@phdthesis{rottet_language_1995,
	address = {Bloomington, IN},
	type = {{PhD}},
	title = {Language shift and language death in the {Cajun} {French}-speaking communities of {Terrebonne} and {Lafourche} parishes, {Louisiana}},
	copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
	abstract = {The attention of linguists has only recently been drawn to the phenomenon of declining minority languages, and the field of inquiry has begun to prove to be a very fruitful one. Perhaps most importantly, dying languages change very quickly, thus providing a unique opportunity for the linguist to study language change. Furthermore, it is possible that some of the changes experienced by dying languages are "unnatural", which has important implications for how linguists handle data from such languages. In this study I examine a Cajun French-speaking community in the context of the study of language decline. This study is valuable not only in its contribution to research on language death in general, but also for its contribution to what is known of Cajun French, a little-studied variety in rapid decline which must therefore be documented soon or not at all. The research involved fieldwork in which I interviewed and taped over 80 speakers of Cajun French in the fall of 1993 and spring 1994, and transcribed these interviews. The study includes two components, a social one and a linguistic one. In the social component I examine changes across time in the patterns of usage of French and English in Louisiana, and examine some of the social correlates of language choice among bilinguals in Louisiana, such as the ethnicity (Native American versus Cajun French), sex, and age of the speaker. In the linguistic component I examine several aspects of the grammar of Cajun French in order to explore the new variety of the language which is emerging as the language declines. The emergent variety differs in some striking ways from that of the traditional norms of the community, and my study serves both to confirm some of the findings from studies of other declining languages, and to report some new patterns the details of which are not attested elsewhere.},
	language = {English},
	urldate = {2016-10-18},
	school = {University of Indiana},
	author = {Rottet, Kevin J.},
	year = {1995},
	keywords = {sociolinguistics, language change},
	file = {Rottet - 1995 - Language shift and language death in the Cajun Fre.pdf:C\:\\Users\\josh8\\Zotero\\storage\\7HDF596H\\Rottet - 1995 - Language shift and language death in the Cajun Fre.pdf:application/pdf},
}

@phdthesis{smith_morphosyntactic_1994,
	address = {Seattle, WA},
	type = {{PhD}},
	title = {A morphosyntactic analysis of the verb group in {Cajun} {French}},
	copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
	url = {http://search.proquest.com.proxy.bibliotheques.uqam.ca:2048/docview/304144741/abstract/9961561190054A43PQ/1},
	abstract = {Languages from differing language families spoken in enclave communities in various parts of the world have been found to exhibit similarities in what is viewed as a 'simplification' of their structures vis-a-vis their corresponding standard dialects (Maher 1985). One of the aspects considered to contribute to a more 'simplified' structure is the replacement of synthetic structures by analytic ones. Analyticity and syntheticity have played a role in language typology, and the cycle is of particular interest in the Romance languages, where Latin is considered the synthetic language par excellence, while French is generally regarded as being highly analytic in comparison. Schwegler (1990) redefines analytic and synthetic, predicating these notions on the spoken, as opposed to the written, language.
This dissertation analyzes the verb group, consisting of the subject and object clitics and the verb, of Cajun French, an enclave language spoken in Louisiana, that has been isolated from the influence of Standard French for some 200 years. The analysis is based on Schwegler's (1990) new definitions of analyticity and syntheticity, which are especially appropriate for this dialect because it has no written form. The data reveal that Cajun French shares many common features with Informal Spoken French, and the net result is that Cajun French is far more synthetic than an analysis based on the written standard would yield.},
	language = {English},
	urldate = {2016-10-18},
	school = {University of Washington},
	author = {Smith, Jane S.},
	year = {1994},
	keywords = {sociolinguistics, language change},
	file = {Smith - 1994 - A morphosyntactic analysis of the verb group in Ca.pdf:C\:\\Users\\josh8\\Zotero\\storage\\ZSKBDGEH\\Smith - 1994 - A morphosyntactic analysis of the verb group in Ca.pdf:application/pdf},
}

