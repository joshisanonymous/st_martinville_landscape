	pages = {135--162},
	file = {Detges - 2000 - Two types of restructuring in French creoles A co.pdf:C\:\\Users\\josh8\\Zotero\\storage\\JHVSSUCJ\\Detges - 2000 - Two types of restructuring in French creoles A co.pdf:application/pdf},
