@article{tarnawska-kaczorowska_musical_1998,
	title = {Musical quotation an outline of the problem},
	volume = {17},
	issn = {0749-4467},
	url = {http://dx.doi.org/10.1080/07494469800640211},
	doi = {10.1080/07494469800640211},
	abstract = {A primary issue: musical tradition, i.e. a work of music as repository of the past. Involved in this are two principal technical-aesthetic-axiological-semiotic types: music in music and music about music. There are various ways of exploting pre-existent material (ante-facti or prae-facti) or stylistic ideas. Governing these are the main sources of borrowings; the polysemic complexity of interrelations and dialogues between loan-music and the music absorbing it (a dialectical network). Musical quotations have various functions, their context an essential factor in conditioning signification: Some quotations are context-sensitive, some context-free. There are negative and positive reasons for employing quotations, and several types of quotation: parole quotation, langue quotation, quotation-symbol, crypto-quotation, hetero-codal quotation, quotation-signature, as well as collage. Numerous illustrations are given, mainly from 20th-century music.},
	number = {3},
	urldate = {2014-04-28},
	journal = {Contemporary Music Review},
	author = {Tarnawska-Kaczorowska, Krystyna},
	year = {1998},
	pages = {69--90},
	file = {Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\R7VBYUWK\\07494469800640211.html:text/html;Tarnawska-Kaczorowska - 1998 - Musical quotation an outline of the problem.pdf:C\:\\Users\\josh8\\Zotero\\storage\\HD6E8UJJ\\Tarnawska-Kaczorowska - 1998 - Musical quotation an outline of the problem.pdf:application/pdf},
