	keywords = {sociolinguistics, language change},
	pages = {235--249},
	file = {Rottet - 2006 - Interrogative pronouns in Louisiana Creole and the.pdf:C\:\\Users\\josh8\\Zotero\\storage\\754FA7Z2\\Rottet - 2006 - Interrogative pronouns in Louisiana Creole and the.pdf:application/pdf},
