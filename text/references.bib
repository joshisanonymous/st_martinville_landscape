@phdthesis{tornquist_attitudes_2000,
	address = {Lafayette, LA},
	type = {{PhD}},
	title = {Attitudes linguistiques vis -à -vis du vernaculaire franc -louisianais dans les programmes d'immersion en {Louisiane}},
	copyright = {Database copyright ProQuest LLC; ProQuest does not claim copyright in the individual underlying works.},
	abstract = {Depuis le début du 19e siècle, le français s'est vu progressivement remplacé par l'anglais, langue officielle qui s'utilise dans les domaines prestigieux de la vie sociale en Louisiane. Depuis le début du 20e siècle, on constate une régression notable de la vitalité des variétés vernaculaires du français en Acadiana, la région où habite la majorité des habitants d'origine francophone. Les années cinquante ont néanmoins attesté un renouveau d'intérêt dans l'héritage culturel et linguistique francophone.
Ce renouveau culturel a abouti à un renouveau linguistique. En 1968, le Conseil pour le Développement du Français en Louisiane (le CODOFIL) a été créé. Le CODOFIL a pour mission de réintroduire le français dans le système scolaire et espère ainsi restaurer le bilinguisme français/anglais en Louisiane. Au début du CODOFIL, l'enseignement du français se limitait à des cours de langue seconde. Les élèves n'acquérraient pas cependant de compétence fonctionnelle communicative en français. Depuis les années 80, des programmes d'immersion ont été créés pour assurer la création de jeunes Louisianais fonctionnellement bilingues.
Depuis sa fondation, le CODOFIL fait la promotion d'une norme officielle, le français standard, qui diffère de la norme communautaire, le français cadien et créole. En effet, jusqu'à récemment, la question de l'enseignement du vernaculaire avait été largement occultée du discours sur l'aménagement du français. Depuis les années quatre vingt, cependant, certains administrateurs font un effort de louisianifier l'éducation linguistique. Les enseignants sont actuellement encouragés à intégrer le vocabulaire franco-louisianais dans leur enseignement.
Cette étude a cherché à connaître les attitudes linguistiques des participants à l'immersion afin de savoir à quel point la louisianification du progranune est possible. Les résultats ont montré que la louisianification limitée de l'éducation linguistique est possible en Acadiana. Les participants à l'immersion, quoique réticents à exprimer leurs attitudes, estiment quil est possible d'enseigner le vernaculaire. Ils sont surtout en faveur de la promotion du vocabulaire franco-louisianais lorsqu'il s'agit de la culture. Pour réussir la louisianification de l'immersion, un projet de conscientisation, comme celui déjà en place pour les enseignants, doit se faire auprés des parents et des administrateurs afin d'améliorer leurs attitudes par rapport au vernaculaire et à sa promotion scolaire.},
	language = {Français},
	urldate = {2022-01-30},
	school = {University of Louisiana Lafayette},
	author = {Tornquist, Lisa Linnea},
	year = {2000},
	keywords = {sociolinguistics, language change},
	file = {Tornquist - 2000 - Attitudes linguistiques vis -à -vis du vernaculair.pdf:C\:\\Users\\josh8\\Zotero\\storage\\T9VC2LD9\\Tornquist - 2000 - Attitudes linguistiques vis -à -vis du vernaculair.pdf:application/pdf},
}

@article{sneller_phonological_2020,
	title = {Phonological rule spreading across hostile lines: ({TH})-fronting in {Philadelphia}},
	volume = {32},
	issn = {0954-3945, 1469-8021},
	shorttitle = {Phonological rule spreading across hostile lines},
	url = {https://www.cambridge.org/core/journals/language-variation-and-change/article/phonological-rule-spreading-across-hostile-lines-thfronting-in-philadelphia/6C0FC55738C87073807B11B1446A7951},
	doi = {10.1017/S0954394519000140},
	abstract = {This article examines the transfer of (TH)-fronting, a phonological feature of African American English, into the speech of white speakers from South Philadelphia. While most cases of linguistic diffusion, particularly of African American English, are found in speakers with a positive affiliation with the source dialect (e.g., Bucholtz, 1999; Cutler, 1999; Fix, 2010), here the white adopters of (TH)-fronting exhibit overtly hostile attitudes toward black neighbors. I argue that (TH)-fronting has been adopted as an index of street or masculinity by the white speakers in this study. Furthermore, I demonstrate that the phonological constraints on borrowed (TH)-fronting have been restricted and simplified from the constraints in the source dialect. Finally, I discuss how hostile interactions may play a role in language change, allowing change in dialects not only to proceed in tandem across unexpected boundaries (e.g., Milroy \& Milroy, 1985), but also to be directly diffused across hostile boundaries.},
	language = {en},
	number = {1},
	urldate = {2022-01-30},
	journal = {Language Variation and Change},
	author = {Sneller, Betsy},
	month = mar,
	year = {2020},
	note = {Publisher: Cambridge University Press},
	keywords = {sociolinguistics, language change},
	pages = {25--47},
	file = {Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\8PE6V7CY\\6C0FC55738C87073807B11B1446A7951.html:text/html;Sneller - 2020 - Phonological rule spreading across hostile lines .pdf:C\:\\Users\\josh8\\Zotero\\storage\\X5SA3W2Z\\Sneller - 2020 - Phonological rule spreading across hostile lines .pdf:application/pdf},
}

