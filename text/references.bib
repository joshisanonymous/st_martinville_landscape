	journal = {Journal of Psycholinguistic Research},
	author = {Leikin, Mark and Ibrahim, Raphiq and Eviatar, Zohar and Sapir, Shimon},
	month = oct,
	year = {2009},
	pages = {447--457},
}

@article{tsukada_cross-language_2006,
	title = {Cross-{Language} {Perception} of {Word}-{Final} {Stops} in {Thai} and {English}},
