@article{baranowski_class_2017,
	title = {Class matters: the sociolinguistics of goose and goat in {Manchester} {English}},
	volume = {29},
	issn = {0954-3945, 1469-8021},
	shorttitle = {Class matters},
	url = {https://www.cambridge.org/core/journals/language-variation-and-change/article/class-matters-the-sociolinguistics-of-goose-and-goat-in-manchester-english/62FFD1216A2063241AECA4003E86D27E},
	doi = {10.1017/S0954394517000217},
	abstract = {This paper reports on patterns of sociolinguistic variation and change in Manchester's goose and goat vowels on the basis of the acoustic analysis of 122 speakers, stratified by age, gender, socioeconomic status, and ethnicity. Goose fronting is an internal change showing little social differentiation, except before /l/ as in school and pool, where, in contrast to most other dialects of English, goose shows advanced fronting inversely correlated with socioeconomic status. Goat fronting, on the other hand, is a change brought from outside the dialect by the highest status groups, displaying a pattern of monotonic social stratification, a female lead, and a strong effect of ethnicity. The role of attitudes toward the community in the realization of the vowels is compared with the effect of social class construed in terms of distances between social groups. Social class turns out to be a better predictor, suggesting that the role of attitudes and identity may be overestimated in research eschewing a systematic exploration of social class at the same time.},
	language = {en},
	number = {3},
	urldate = {2020-08-19},
	journal = {Language Variation and Change},
	author = {Baranowski, Maciej},
	month = oct,
	year = {2017},
	note = {Publisher: Cambridge University Press},
	keywords = {sociolinguistics, language change},
	pages = {301--339},
	file = {Baranowski - 2017 - Class matters the sociolinguistics of goose and g.pdf:C\:\\Users\\josh8\\Zotero\\storage\\T4QKWDEH\\Baranowski - 2017 - Class matters the sociolinguistics of goose and g.pdf:application/pdf;Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\PT4QQ62Q\\62FFD1216A2063241AECA4003E86D27E.html:text/html},
}

@article{delforge_nobody_2012,
