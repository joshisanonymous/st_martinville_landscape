@article{leon_systeme_1973,
	title = {Le système de salutations en milieu intra-familial rural dans la région de {Chinon}},
	volume = {3},
	issn = {0046-2616},
	url = {https://www.jstor.org/stable/40988263},
	abstract = {[On étudie en général la formule de salutation en partant du principe que lorsqu'un individu ouvre ou ferme une communication orale avec un autre individu, le patron linguistique qu'il utilise est porteur d'informations sur la qualité sociale et affective de leur relation. Cet article se propose de chercher si le système des salutations dans la culture étudiée n'a pas, outre la valeur informative que nous venons de mentionner, une valeur hiérarchisante visant à maintenir l'autorité parentale. De l'analyse exhaustive du corpus se dégage un modèle d'adresse sous forme de syntagme ainsi qu'un modèle du fonctionnement de son échange. Mais on s'aperçoit que le rendement des informations diffusées par ces deux modèles est nul. On se demande alors si à la lumière de textes de sociologues, psychologues et philosophes contemporains le système d'adresse dans sa force apodictique ne peut être envisagé comme une sorte de rituel destiné à perpétuer la domination des parents. Systems of address are usually studied on the premisses that each time an individual wants to open or close a speech communication with another individual or a group, the linguistic pattern he uses reflects the social and affective quality of their relationship. A thorough analysis of the data leads to the generation of a model of address as well as that of a model of exchange. But one finds evidence that the output of informations released by this two models is close to nothing. Thus the question one may ask itself is : is there any occulted value underlying the all structure ? Recent research dealing with sociology, psychoanalysis and philosophy shows that the system of address could be seen as a kind of ritual which would assess the parental authority.]},
	language = {French},
	number = {3/4},
	urldate = {2019-05-01},
	journal = {Ethnologie française},
	author = {Léon, Monique},
	year = {1973},
	keywords = {sociolinguistics, language change},
	pages = {329--346},
	file = {Léon - 1973 - Le système de salutations en milieu intra-familial.pdf:C\:\\Users\\josh8\\Zotero\\storage\\TZTUTN8A\\Léon - 1973 - Le système de salutations en milieu intra-familial.pdf:application/pdf},
