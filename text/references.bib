@article{chodroff_structure_2017,
	title = {Structure in talker-specific phonetic realization: {Covariation} of stop consonant {VOT} in {American} {English}},
	volume = {61},
	issn = {0095-4470},
	shorttitle = {Structure in talker-specific phonetic realization},
	url = {http://www.sciencedirect.com/science/article/pii/S0095447017300013},
	doi = {10.1016/j.wocn.2017.01.001},
	abstract = {Variation across talkers in the acoustic-phonetic realization of speech sounds is a pervasive property of spoken language. The present study provides evidence that variation across talkers in the realization of American English stop consonants is highly structured. Positive voice onset time (VOT) was examined for all six word-initial stop categories in isolated productions of CVC syllables and in a multi-talker corpus of connected read speech. The mean VOT for each stop differed considerably across talkers, replicating previous findings, but importantly there were strong and statistically significant linear relations among the means (e.g., the mean VOTs of [pʰ] and [kʰ] were highly correlated across talkers, r{\textgreater}0.80). The pattern of VOT covariation was not reducible to differences in speaking rate or other factors known to affect the realization of stop consonants. These findings support a uniformity constraint on the talker-specific realization of a phonetic property, such as glottal spreading, that is shared by multiple speech sounds. Because uniformity implies mutual predictability, the findings also shed light on listeners׳ ability to generalize knowledge of a novel talker from one stop consonant to another. More broadly, structured variation of the kind investigated here indicates a relatively low-dimensional encoding of talker-specific phonetic realization in both speech production and speech perception.},
	urldate = {2018-10-12},
	journal = {Journal of Phonetics},
	author = {Chodroff, Eleanor and Wilson, Colin},
	month = mar,
	year = {2017},
	keywords = {phonetics, linguistics, phonology},
	pages = {30--47},
	file = {Chodroff et Wilson - 2017 - Structure in talker-specific phonetic realization.pdf:C\:\\Users\\josh8\\Zotero\\storage\\UZY5XHX6\\Chodroff et Wilson - 2017 - Structure in talker-specific phonetic realization.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\josh8\\Zotero\\storage\\SK4FU8GE\\S0095447017300013.html:text/html},
}

@article{landry_linguistic_1997,
	title = {Linguistic {Landscape} and {Ethnolinguistic} {Vitality}: {An} {Empirical} {Study}},
	volume = {16},
	issn = {0261-927X},
	shorttitle = {Linguistic {Landscape} and {Ethnolinguistic} {Vitality}},
	url = {https://doi.org/10.1177/0261927X970161002},
	doi = {10.1177/0261927X970161002},
	abstract = {Linguistic landscape refers to the visibility and salience of languages on public and commercial signs in agiven territory or region. It is proposed that the linguistic landscape may serve important informational and symbolic functions as a marker of the relative power and status of the linguistic communities inhabiting the territory. Using the theoretical framework of ethnolinguistic vitality, it was hypothesized that the experience of the linguistic landscape by members of a language group may contribute to social psychological aspects of bilingual development. Factor analysis results show that the linguistic landscape emerges as a distinct factor separate from other measures of linguistic contacts. This factor was an important correlate of subjective ethnolinguistic vitality representing perceptions of the vitality of the in-group language in various domains. The study also found relations between the Linguistic Landscape factor and degree of in-group language use, especially in institutional settings, suggesting a 'carryover effect" of the linguistic landscape on language behavior.},
	language = {English},
	number = {1},
	urldate = {2018-10-11},
